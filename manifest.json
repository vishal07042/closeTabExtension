{
  "manifest_version": 3,
  "name": "DSA Quiz New Tab",
  "version": "1.0",
  "description": "Solve DSA questions before browsing",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  }
} 